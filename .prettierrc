{
    "semi": true,
    "endOfLine": "auto",
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "importOrder": [
      "^css\\/global\\.scss$",
      "^(api-clients|components|constants|assets|css|i18n|pages|redux|tests|interfaces|hooks|utils)\\/(?!global\\.scss).*|(routes|config|i18n)$",
      "^(\\.\\/|\\.\\.\\/)((?!\\.module\\.scss).)*$",
      ".*\\.module\\.scss$"
    ],
    "importOrderSeparation": true,
    "useTabs": false
  }
  